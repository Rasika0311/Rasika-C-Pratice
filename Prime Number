Prime Number

#include<iostream>
using namespace std;
int main()
{
    int i,num,count=0;
    cin>>num;//3
    if(num==0)
    {
        cout<<"Not Prime";
    }
    else
    {
        for(i=2;i<num;i++)//2
        {
            if(num%i==0)
            {
                count++;
            }
            
        }
        
    }
    if(count<1)
    {
        cout<<"Prime";
    }
    else
    {
        cout<<"Not prime";
    }
}
